{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "rewrites": [
    {
      "source": "/socket.io/:path*",
      "destination": "https://fixmybug-backend.vercel.app/socket.io/:path*"
    }
  ]
}
